{
  "name": "deployer",
  "version": "1.0.0",
  "description": "Vercel deployment tool with custom subdomain configuration",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/server/index.ts",
    "build": "tsc",
    "start": "npm run build && node dist/server/index.js",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "dotenv": "^16.6.1",
    "express": "^5.0.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^22.10.5",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3"
  }
}
